{
    "name": "nodes/api",
    "description": "Nodes wrapper for Dingo API",
    "keywords": [
        "nodes",
        "api",
        "laravel",
        "dingo"
    ],
    "license": "MIT",
    "homepage": "http://nodesagency.com",
    "authors": [
        {
            "name": "Morten Rugaard",
            "email": "moru@nodes.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "5.1.*||5.2.*",
        "nodes/core": "^0.1",
        "nodes/bugsnag": "^0.1",
        "nodes/database": "^0.1",
        "nodes/validation": "^0.1",
        "nodes/cache": "^0.1",
        "dingo/api": "1.0.x@dev"
    },
    "autoload": {
        "psr-4": {
            "Nodes\\Api\\": "src/"
        },
        "files": [
            "src/Support/Helpers/Auth.php",
            "src/Support/Helpers/Manipulation.php",
            "src/Support/Helpers/Route.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php -f vendor/nodes/core/install.php nodes:package:install nodes/api"
        ],
        "post-update-cmd": [
            "php -f vendor/nodes/core/install.php nodes:package:install nodes/api"
        ]
    }
}